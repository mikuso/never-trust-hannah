"UnlitGeneric"
{
    "$basetexture" "nth/posters/meowoclock"
    "$vertexcolor" 1
    "$vertexalpha" 1
    "$translucent" 1
    "$nolod" 1
    "Proxies"
    {
        "AnimatedTexture"
        {
            "animatedtexturevar" "$basetexture"
            "animatedtextureframenumvar" "$frame"
            "animatedtextureframerate" "4.3"
        }
    }
}
